{
  "compilerOptions": {
    "module": "es2015",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "types": [
      "jasmine"
    ],
    "typeRoots": [
      "../node_modules/@types"
    ]
  },
  "awesomeTypeScriptLoaderOptions": {
    "useWebpackText": true
  }
}
